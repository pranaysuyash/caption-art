    "node-fetch": "^2.7.0",
    "undici": "^7.16.0",
    "zod": "^4.1.13"
  },
  "optionalDependencies": {
    "pino": "^8.17.0",
    "pino-http": "^8.1.0",
    "uuid": "^9.0.0",
    "pg": "^8.11.0",
    "@prisma/client": "^6.19.0",
    "@prisma/internals": "^7.1.0",
